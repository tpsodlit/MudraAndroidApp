<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.collection.tpwodloffline.CollSetupMenuActivity"

    tools:showIn="@layout/activity_coll_setup_menu">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:gravity="center"
        android:orientation="vertical">

        <Button
            android:id="@+id/printertye"
            android:layout_width="312dp"
            android:layout_height="61dp"
            android:layout_marginTop="10dp"
            android:background="#6183c3"
            android:text="PRINTER SETUP"
            android:textSize="20dp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/switch_url"
            android:layout_width="312dp"
            android:layout_height="61dp"
            android:layout_marginTop="10dp"
            android:background="#6183c3"
            android:text="SWITCH URL SETUP"
            android:textSize="20dp"
            android:textStyle="bold" />
    </LinearLayout>
</RelativeLayout>
